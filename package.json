{
  "name": "auth-spa",
  "version": "1.0.16",
  "description": "",
  "private": true,
  "workspaces": [
    "Auth-SPA-Client",
    "Auth-SPA-API"
  ],
  "scripts": {
    "client": "NODE_ENV=development yarn workspace auth-spa-client serve",
    "client-test": "yarn workspace auth-spa-client test",
    "client-deploy": "yarn workspace auth-spa-client deploy",
    "server": "NODE_ENV=development yarn workspace auth-spa-api start",
    "server-test": "yarn workspace auth-spa-api test",
    "server-deploy": "yarn workspace auth-spa-api deploy",
    "start": "concurrently --kill-others-on-fail \"yarn server\"  \"yarn client\"",
    "test": "concurrently \"yarn server-test\"  \"yarn client-test\"",
    "deploy": "concurrently \"yarn server-deploy\"  \"yarn client-deploy\""
  },
  "repository": {
    "type": "git"
  },
  "keywords": [],
  "author": "JT Houk <jt1992@gmail.com> (https://jt.houk.space/)",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^5.0.0"
  }
}
